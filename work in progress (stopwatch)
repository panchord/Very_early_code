import time
seconds = int(0)
minutes = int(0)


def ticking():
    time.sleep(1)


def sec(x):
    x = x + 1
    if x == 60:
        x = 0
    return x


def button():



while seconds < 99:
    ticking()
    seconds = sec(seconds)
    print(minutes, ":", seconds)
    if input("s"):
        button(seconds)
