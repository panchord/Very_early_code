n1 = float(input("Type your first number: "))
n2 = float(input("Type your : "))
sum = n1*n2
if sum >= int(99999):
    for x in range(10):
        print(sum, "!!! So close to infinityyyyyyyy!!!!!!!!")
elif sum >= int(9999):
    print("Woah,", sum, "! That's a REALLY big number!")
elif sum >= int(999):
    print("Woah,", sum, "!!! That's a big number!")
else:
    print(sum)
    
